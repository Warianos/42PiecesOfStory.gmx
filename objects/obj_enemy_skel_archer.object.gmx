            <string>//turn the skeleton to face the player    
if(obj_player.y &lt;= y &amp;&amp; obj_player.x &gt;= x)
    radians = arctan((y - obj_player.y) / (obj_player.x - x))
else if(obj_player.y &gt;= y &amp;&amp; obj_player.x &lt;= x)
    radians = pi - arctan((obj_player.y - y)/(obj_player.x - x))
else if(obj_player.y &gt;= y &amp;&amp; obj_player.x &gt;= x)
    radians = arctan((obj_player.y - y)/(x - obj_player.x))
else   
    radians = pi - arctan((obj_player.y - y)/(obj_player.x - x))
     
direction = radtodeg(radians)

switch(state){
    case(enemy_state.stop):
    {
        vspeed = 0
        hspeed = 0
        if(canAttack)
            state = enemy_state.chase
    }
    break;
    case(enemy_state.chase):
    {
        if(canAttack){
            arrow = instance_create(x, y, obj_enemy_arrow)
            arrow.speed = max_fps/5
            arrow.direction = direction
            canAttack = false
        }
        state = enemy_state.stop
        alarm[4] = max_fps/10
    }
    break;
}
</string>
