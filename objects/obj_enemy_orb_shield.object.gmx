if(mage.currentStage == "wolves")
{
    if(mage.hoverAscending)
    {
        x = x + mage.hoverFactorX / room_speed
        y = y - mage.hoverFactorX / room_speed
    }
    else
    {
        x = x - mage.hoverFactorX / room_speed
        y = y + mage.hoverFactorX / room_speed
    } 
